package main

func addition (number1 i32, number2 i32) (result i32) {
    result = number1 + number2
}

type Player struct {
    name str
}

type Referee struct {
    name str
}

func (p *Player) printName () {
    str.print("Player information")
    str.print((*p).name)
}

func (r *Referee) printName () {
    str.print("Referee information")
    str.print((*r).name)
}

func main () {
    sum := addition(3,4)

    i32.print(sum)

    var p Player
    p.name = "Michael"
    
    var r Referee
    r.name = "Edward"

    p.printName()
    r.printName()
}